{
  "productName": "kreck",
  "version": "0.0.0",
  "identifier": "com.kreck.dev",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "kreck",
        "width": 260,
        "height": 250,
        "skipTaskbar": true,
        "resizable": false,
        "minimizable": false,
        "alwaysOnTop": true,
        "visible": false,
        "focus": false,
        "closable": false,
        "hiddenTitle": true,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
