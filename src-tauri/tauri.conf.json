{
  "productName": "kreck",
  "version": "0.0.0",
  "identifier": "com.kreck.dev",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "trayIcon": {
      "iconPath": "./icons/icon.ico",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "title": "kreck",
        "width": 400,
        "height": 300,
        "skipTaskbar": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
